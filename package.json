{
  "name": "lexermal_mediumnewsbot",
  "version": "0.1.0",
  "main": "bin/index.js",
  "description": "A Node.js Telegram Bot to subscribe Medium articles for reading",
  "repository": "https://github.com/lexermal/mediumNewsBot",
  "author": "Lexermal",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "dotenv": "16.0.1",
    "logform": "2.4.2",
    "node-html-parser": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "rss-parser": "^3.7.0",
    "sqlite3": "^5.0.2",
    "telegraf": "^3.27.1",
    "typeorm": "^0.2.41",
    "typescript": "4.7.4",
    "winston": "3.8.1"
  },
  "devDependencies": {
    "@types/node": "18.7.6",
    "ts-node": "10.9.1"
  },
  "scripts": {
    "start": "yarn prod",
    "dev": "yarn install && export FETCHING_DURATION=0.9 && export SENDING_DURATION=0.3 && yarn prod",
    "prod": "yarn ts-node --require dotenv/config ./src/index.ts"
  },
  "keywords": [
    "Medium",
    "Telegram",
    "Bot"
  ],
  "bugs": {
    "url": "https://github.com/lexermal/mediumNewsBot/issues"
  },
  "homepage": "https://github.com/lexermal/mediumNewsBot#readme"
